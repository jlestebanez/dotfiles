cdp () {
 cd /Users/joseluis/Development/proyectos/$1
}
_cdp ()
{
    local cur
    COMPREPLY=()
    cur=${COMP_WORDS[COMP_CWORD]}
    case "$cur" in
        *)
            pushd "/Users/joseluis/Development/proyectos/">/dev/null
            COMPREPLY=( $(compgen -d $cur) )
            ;;
    esac

    return 0
} &&
complete -o filenames -F _cdp cdp
